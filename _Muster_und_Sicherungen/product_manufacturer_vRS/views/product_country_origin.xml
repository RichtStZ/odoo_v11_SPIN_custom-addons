<?xml version="1.0" ?>
<odoo>

    <record id="product_template_country_of_origin_form_view" model="ir.ui.view">
        <field name="name">product.template.form - product_country_origin</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="delivery.product_template_hs_code"/>
        <field name="arch" type="xml">
            <data>
                <field name="hs_code" position="after">
                    <field name="country_of_origin_id"/>
                    <field name="country_of_origin_id.code"/>
                </field>
            </data>
        </field>
    </record>

</odoo>
