<?xml version="1.0" ?>
<odoo>

    <record id="product_template_country_of_origin_form_view" model="ir.ui.view">
        <field name="name">product.template.form - product_country_origin</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='uom_po_id']" position="after">
                <field name="country_of_origin" string="Ursprungsland"/>
                <!--
                <field name="country_of_origin.code"/>
                -->

            </xpath>
        </field>
    </record>

</odoo>
